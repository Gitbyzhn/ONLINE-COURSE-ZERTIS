

@{
    ViewBag.Title = "Quiz";
    ViewBag.Quiz = "active";
    Layout = "~/Views/Shared/_Layout_portal.cshtml";
    ViewBag.Quiz = "Опрос";
}









@section data_table_css{
    
<link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css">
<link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.6.2/css/buttons.dataTables.min.css">
    
}




<section class="content-header">
    <h1>
        Таблица покупатели продуктов
    </h1>




    <ol class="breadcrumb">
        <li><a href="@Url.Action("index","portal")"><i class="fa fa-dashboard"></i> Главная</a></li>

        <li class="active">                     ОПРОСЫ А</li>
    </ol>



</section>
<!-- Main content -->
<section class="content">
    <div class="row">

        <div class="col-xs-12">

            <div class="box" style="margin-top:20px">

                <!-- /.box-header -->
                <div class="box-body">

                    <style>

                        #data_table_wrapper {
                          overflow:auto;
                        }
                    </style>



                    <table id="data_table" class="table table-bordered table-striped" style="overflow:auto">
                        <thead>
                            <tr>

                               
                                <th>Тел.Номер</th>
                                <th>Пользователь</th>
                                <th>1.Как вы узнали о нашем курсе?</th>
                                <th>2.С какой целью вы решили пройти данный учебный курс?</th>
                                <th>3.Оцените данный учебный курс в целом</th>
                                <th>4.Насколько доступно и понятно было представление информации</th>
                                <th>5.Насколько актуально и полезно было содержания курса для вас</th>
                                <th>6.Достаточно ли вы получили дополнительного материала </th>
                                <th>7.Приобрели ли вы новые навыки </th>
                                <th>8.Оцените удобство пользования сайтом</th>
                                <th>9.Оцените качество обслуживания (поддержка при регистрации)</th>
                                <th>10.Что бы вы посоветовали для улучшения качества обслуживания и сайта</th>
                                <th>11.Что бы вы посоветовали для улучшения содержания курса?</th>
                                <th>12.Назовите основные сильные стороны этого курса?</th>
                                <th>13.Назовите основные недостатки этого курса?</th>
                                <th>14.Другие комментарии и предложения </th>

                            </tr>
                        </thead>
             
                        <tfoot>
                        
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>
</section>








@section data_table_js{
    <!-- DataTables -->




    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>

    <script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>

    <script src="https://cdn.datatables.net/buttons/1.6.2/js/dataTables.buttons.min.js"></script>

    <script src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.flash.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>


    <script src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.html5.min.js"></script>









}




@section data_table_js_quiz{

<script>


    var data = {language:"ru"};

    $.ajax({

        type: "GET",
        url: '/portal/UsersQuiz',
        datatype: 'json',
        data:data,
        success: function (obj, textstatus) {

            $('#data_table').DataTable({
                data: obj,
                dom: 'Bfrtip',
                buttons: [
                'excel',
                ],
                "language": {
                    "url": "//cdn.datatables.net/plug-ins/1.10.19/i18n/Russian.json"
                },

            });


        },
        error: function (obj, textstatus) {
            alert(obj.msg);
        }
    });

</script>

}