

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title> Онлайн курс | ZERTIS @ViewBag.Quiz</title>

    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/portal/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/portal/bower_components/font-awesome/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="~/Content/portal/bower_components/Ionicons/css/ionicons.min.css">
    <!-- DataTables -->
  

    @if (IsSectionDefined("data_table_css"))
    {
        @RenderSection("data_table_css");
    }



    
     <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/portal/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/portal/dist/css/skins/_all-skins.min.css">

    <!-- bootstrap wysihtml5 - text editor -->
    <link rel="stylesheet" href="~/Content/portal/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css">

    <link href="~/Content/css/private/Admin.css" rel="stylesheet" />

    <!-- iCheck -->
    <link rel="stylesheet" href="~/Content/portal/plugins/iCheck/flat/blue.css">

    <!-- Toastr -->
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">

    <!-- Portal General CSS -->
    <link href="~/Content/portal/mycss/generalcss.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">



    <!-- ==============================================
    Favicons
    =============================================== -->
    <link rel="apple-touch-icon" sizes="76x76" href="~/Content/img/favicon.ico">
    <link rel="icon" type="image/png" href="~/Content/img/favicon.ico">

</head>


<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>O</b>Z</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>ONLINE</b> ZERTIS</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">




                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">





                                <img src="~/Images/default-avatar.png" class="user-image" alt="User Image">




                                <span class="hidden-xs">Админ MH</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">






                                    <img src="~/Images/default-avatar.png" class="img-circle" alt="User Image">


                                    <p>
                                        Админ MH

                                    </p>
                                </li>

                                <!-- Menu Footer-->
                                <li class="user-footer">

                                    @if (Request.IsAuthenticated)
                                    {
                                        using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @style = " margin-right: 14px;" }))
                                        {
                                            @Html.AntiForgeryToken()



                                            <div class="pull-right">
                                                <a href="javascript:document.getElementById('logoutForm').submit()" class="btn btn-default btn-flat">Выйти</a>
                                            </div>


                                        }
                                    }


                                </li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel" style="height: 80px">
                    <div class="pull-left image">




                        <img src="~/Images/default-avatar.png" class="img-circle" alt="User Image">

                    </div>
                    <div class="pull-left info">
                        <p> Админ MH</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>


                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">ОСНОВНАЯ НАВИГАЦИЯ</li>

                    <li class="@ViewBag.Modules">
                        <a href="@Url.Action("Modules","portal")">
                            <i class="fa fa-tasks"></i> <span>Модули</span>

                        </a>
                    </li>

                    <li class="@ViewBag.VideoLessons">
                        <a href="@Url.Action("VideoLessons","portal")">
                            <i class="fa fa-file-movie-o"></i> <span>ВИДЕО УРОКИ</span>

                        </a>
                    </li>
                    <li class="@ViewBag.VideoTests">
                        <a href="@Url.Action("VideoTests","portal")">
                            <i class="fa fa-file-o"></i> <span>ВИДЕО ТЕСТЫ</span>

                        </a>
                    </li>


                    <li class="@ViewBag.Buyers">
                        <a href="@Url.Action("buyers","portal")">
                            <i class="fa fa-suitcase"></i> <span>ПОКУПАТЕЛИ</span>

                        </a>
                    </li>

                    <li class="@ViewBag.Users">
                        <a href="@Url.Action("Users","portal")">
                            <i class="fa fa-users"></i> <span>ПОЛЬЗОВАТЕЛИ</span>

                        </a>
                    </li>


                    <li class="@ViewBag.Quiz">
                        <a href="@Url.Action("Quiz","portal")">
                            <i class="fa fa-question"></i> <span>ОПРОСЫ</span>

                        </a>
                    </li>


                    <li class="@ViewBag.SetExpiredTimeVL">
                        <a href="@Url.Action("SetExpiredTimeVL","portal")">
                            <i class="fa fa-users"></i> <span>Ограничения</span>

                        </a>
                    </li>

              


                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">

            @RenderBody()


        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.13
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year <a href="https://adminlte.io">Admin ONLINE COURSE ZERTIS</a>.</strong> All rights
            reserved.
        </footer>



    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/Content/portal/bower_components/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/portal/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- Slimscroll -->
    <script src="~/Content/portal/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>


    <!-- InputMask -->
    <script src="~/Content/portal/plugins/input-mask/jquery.inputmask.js"></script>
    <script src="~/Content/portal/plugins/input-mask/jquery.inputmask.date.extensions.js"></script>
    <script src="~/Content/portal/plugins/input-mask/jquery.inputmask.extensions.js"></script>



    <!-- DataTables -->
    @if (IsSectionDefined("data_table_js"))
    {
        @RenderSection("data_table_js");
    }


    <!-- FastClick -->
    <script src="~/Content/portal/bower_components/fastclick/lib/fastclick.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/portal/dist/js/adminlte.min.js"></script>

    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>


    <!-- AdminLTE for demo purposes -->
    <script src="~/Content/portal/dist/js/demo.js"></script>



    <script>

        $(document).ready(function () {



        toastr.options = {
                "closeButton": false,
                "debug": false,
                "newestOnTop": false,
                "progressBar": false,
                "positionClass": "toast-top-right",
                "preventDuplicates": false,
                "onclick": null,
                "showDuration": "300",
                "hideDuration": "1000",
                "timeOut": "5000",
                "extendedTimeOut": "1000",
                "showEasing": "swing",
                "hideEasing": "linear",
                "showMethod": "fadeIn",
                "hideMethod": "fadeOut"
            }


            @if (IsSectionDefined("data_table"))
            {
                @RenderSection("data_table");
            }

            @if (IsSectionDefined("AddVideoMessage"))
            {
                @RenderSection("AddVideoMessage");
            }


            $('#Limitations').click(function () {

                if ($(this).prop("checked") == true) {
                    $("#inpComletedata").prop("disabled", false);
                }
                else if ($(this).prop("checked") == false) {
                 

                    $("#inpComletedata").prop("disabled", true);
                }

            });



        });


        $(function () {
      

            //Datemask dd/mm/yyyy
            $('#datemask').inputmask('dd/mm/yyyy', { 'placeholder': 'dd/mm/yyyy' })
            //Datemask2 mm/dd/yyyy
            $('#datemask2').inputmask('mm/dd/yyyy', { 'placeholder': 'mm/dd/yyyy' })

            //Money Euro
            $('[data-mask]').inputmask()
        });
    </script>



    @if (IsSectionDefined("data_table_js_quiz"))
    {
        @RenderSection("data_table_js_quiz");
    }


</body>
</html>
